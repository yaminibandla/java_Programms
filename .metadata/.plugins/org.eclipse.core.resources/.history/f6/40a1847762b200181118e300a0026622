package programsjava;

public class Starprgrams {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1;i<=5;i++)
		{
			for(int j=2;j>=i;j--)
			{
				System.out.print(" ");
			}
			for(int k=1;k<=(2*i-1);k++)
			{
				System.out.println("*");
				
				
		
			}
			System.out.print(" ");
		}
		
		
		

	}

}
